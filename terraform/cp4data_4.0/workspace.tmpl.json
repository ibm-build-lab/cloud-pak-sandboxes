{
  "name": "{{ WORKSPACE_NAME }}",
  "type": [
    "terraform_v0.13"
  ],
  "description": "IBM Cloud Schematics Workspace to deploy an OpenShift cluster sandbox for Cloud Pak for Data on IBM Cloud Classic",
  "tags": [
    "owner:{{ OWNER }}",
    "env:{{ ENV }}",
    "project:{{ PROJECT }}"
  ],
  "resource_group": "cloud-pak-sandbox-ibm",
  "template_repo": {
    "url": "https://github.com/ibm-hcbt/cloud-pak-sandboxes",
    "branch": "{{ BRANCH }}"
  },
  "template_data": [
    {
      "folder": "terraform/cp4data",
      "type": "terraform_v0.13",
      "variablestore": [
        {
          "name": "region",
          "value": "us-south",
          "type": "string"
        },
        {
          "name": "project_name",
          "value": "{{ PROJECT }}",
          "type": "string"
        },
        {
          "name": "owner",
          "value": "{{ OWNER }}",
          "type": "string"
        },
        {
          "name": "environment",
          "value": "{{ ENV }}",
          "type": "string"
        },
        {
          "name": "resource_group",
          "value": "cloud-pak-sandbox-ibm",
          "type": "string"
        },
        {
          "name": "cluster_id",
          "value": "{{ CLUSTER_ID }}",
          "type": "string"
        },
        {
          "name": "entitlement",
          "value": "cloud_pak",
          "type": "string"
        },
        {
          "name": "datacenter",
          "value": "dal10",
          "type": "string"
        },
        {
          "name": "on_vpc",
          "value": "false",
          "type": "bool"
        },
        {
          "name": "install_portworx",
          "value": "false",
          "type": "bool"
        },
        {
          "name": "vpc_zone_names",
          "value": "[\"us-south-1\"]",
          "type": "list(string)"
        },
        {
          "name": "private_vlan_number",
          "value": "",
          "type": "string"
        },
        {
          "name": "public_vlan_number",
          "value": "",
          "type": "string"
        },
        {
          "name": "storage_class_name",
          "value": "ibmc-file-custom-gold-gid",
          "type": "string"
        },
        {
          "name": "entitled_registry_key",
          "value": "{{ ENTITLED_KEY }}",
          "type": "string",
          "secure": true
        },
        {
          "name": "entitled_registry_user_email",
          "value": "{{ ENTITLED_KEY_EMAIL }}",
          "type": "string"
        },
        {
          "name": "ibmcloud_api_key",
          "value": "",
          "type": "string"
        },
        {
          "name": "storage_capacity",
          "value": "200",
          "type": "number"
        },
        {
          "name": "storage_iops", 
          "value": "10",
          "type": "number"
        },
        {
          "name": "storage_profile",
          "value": "10iops-tier",
          "type": "string"
        },
        {
          "name": "create_external_etcd",
          "value": "false",
          "type": "bool"
        },
        {
          "name": "etcd_username",
          "value": "",
          "type": "string"
        },
        {
          "name": "etcd_password",
          "value": "",
          "type": "string"
        },
        {
          "name": "empty_module_list",
          "value": "true",
          "type": "bool"
        },
        {
          "name": "install_wsl",
          "value": "false",
          "type": "bool"
        },
        {
          "name": "install_aiopenscale",
          "value": "false",
          "type": "bool"
        },
        {
          "name": "install_wml",
          "value": "false",
          "type": "bool"
        },
        {
          "name": "install_wkc",
          "value": "false",
          "type": "bool"
        },
        {
          "name": "install_dv",
          "value": "false",
          "type": "bool"
        },
        {
          "name": "install_spss",
          "value": "false",
          "type": "bool"
        },
        {
          "name": "install_cde",
          "value": "false",
          "type": "bool"
        },
        {
          "name": "install_spark",
          "value": "false",
          "type": "bool"
        },
        {
          "name": "install_dods",
          "value": "false",
          "type": "bool"
        },
        {
          "name": "install_ca",
          "value": "false",
          "type": "bool"
        },
        {
          "name": "install_ds",
          "value": "false",
          "type": "bool"
        },
        {
          "name": "install_db2oltp",
          "value": "false",
          "type": "bool"
        },
        {
          "name": "install_db2wh",
          "value": "false",
          "type": "bool"
        },
        {
          "name": "install_big_sql",
          "value": "false",
          "type": "bool"
        },
        {
          "name": "install_wsruntime",
          "value": "false",
          "type": "bool"
        }
      ]
    }
  ]
}
